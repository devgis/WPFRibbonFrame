<Application x:Class="DEVGIS.Main.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:DEVGIS.Main"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Fluent;component/Themes/Generic.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Fluent;component/Themes/Themes/Light.Blue.xaml" />

                <!--<ResourceDictionary Source="pack://application:,,,/Fluent;component/Themes/Themes/Dark.Green.xaml" />-->
            </ResourceDictionary.MergedDictionaries>

            <Style x:Key="{x:Type CheckBox}"
                   TargetType="CheckBox">
                <Setter Property="Foreground" Value="{DynamicResource BlackBrush}" />
            </Style>
        </ResourceDictionary>
    </Application.Resources>
</Application>
